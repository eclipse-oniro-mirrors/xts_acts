{
  "description": "Configuration for hjunit demo Tests",
  "driver": {
    "type": "OHJSUnitTest",
    "test-timeout": "600000",
    "bundle-name": "com.example.notificationchangetest",
    "module-name": "entry_test",
    "shell-timeout": "600000",
    "testcase-timeout": 70000
  },
  "kits": [
    {
      "test-file-name": [
        "ActsNotificationChangeTest.hap"
      ],
      "type": "AppInstallKit",
      "cleanup-apps": true
    },
    {
      "type": "ShellKit",
      "run-command": [
        "wait 10",
        "power-shell wakeup",
        "uinput -T -m 300 2700 280 200",
        "power-shell setmode 602",
        "setenforce 0"
      ]
    }
  ]
}