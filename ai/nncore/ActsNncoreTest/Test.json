{
    "description": "Configuration for ActsNncoreTest Tests",
    "driver": {
        "type": "OHJSUnitTest",
        "test-timeout": "1800000",
        "shell-timeout": "1800000",
        "bundle-name": "net.openvalley.acts_nn_core_test",
        "module-name": "entry_test"
    },
    "kits": [
        {
            "test-file-name": [
                "ActsNncoreTest.hap",
                "ActsNncoreMainTest.hap"
            ],
            "type": "AppInstallKit",
            "cleanup-apps": true
        },{
            "type": "ShellKit",
            "run-command": [
                "power-shell wakeup",
                "power-shell setmode 602"
            ]
        }
    ]
}
