{
    "description": "Configuration for hjunit demo Tests",
    "driver": {
        "type": "OHJSUnitTest",
        "test-timeout": "200000",
        "bundle-name": "com.example.accounttstest",
        "module-name": "entry_test",
        "shell-timeout": "200000",
        "testcase-timeout": 150000
    },
    "kits": [
        {
            "test-file-name": [
                "ActsAccountTsTest.hap",
                "ActsAccountOauthtoken.hap",
                "ActsAccountAuthenticator.hap"
            ],
            "type": "AppInstallKit",
            "cleanup-apps": true
        },
        {
            "type": "ShellKit",
            "run-command": [
                "power-shell wakeup",
                "power-shell setmode 602",
                "power-shell timeout -o 86400000",
                "hidumper -s 3301 -a -t",
                "param set persist.sys.abilityms.support.ecologicalrulemgrservice false"
            ]
        },
        {
            "type": "ShellKit",
            "teardown-command":[
                "bm uninstall -n com.example.accounttstest"
            ]
        }
    ]
}
