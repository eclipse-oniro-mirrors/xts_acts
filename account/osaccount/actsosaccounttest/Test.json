{
    "description": "Configuration for hjunit demo Tests",
    "driver": {
        "type": "OHJSUnitTest",
        "test-timeout": "3000000",
        "bundle-name": "com.acts.osaccounttest",
        "module-name": "entry_test",
        "shell-timeout": "3000000",
        "testcase-timeout": 50000
    },
    "kits": [
        {
            "test-file-name": [
                "ActsOsAccountEnterpriseTest.hap",
                "ActsOsAccountAssistAppOneEnterpriseTest.hap",
                "ActsOsAccountAssistAppTwoEnterpriseTest.hap"
            ],
            "type": "AppInstallKit",
            "cleanup-apps": true
        },
        {
            "type": "ShellKit",
            "run-command": [
                "power-shell wakeup",
                "power-shell setmode 602",
                "power-shell timeout -o 86400000",
                "hidumper -s 3301 -a -t"
            ]
        },
        {
            "type": "ShellKit",
            "teardown-command":[
                "bm uninstall -n com.acts.osaccounttest"
            ]
        }
    ]
}
