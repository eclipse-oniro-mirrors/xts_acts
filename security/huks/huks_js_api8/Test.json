{
    "description": "Configuration for huks js api Tests",
    "driver": {
        "type": "OHJSUnitTest",
        "test-timeout": "4200000",
        "shell-timeout": "4200000",
        "bundle-name": "com.openharmony.huksapi8",
        "package-name": "com.openharmony.huksapi8",
        "testcase-timeout": 1500000
    },
    "kits": [
        {
            "test-file-name": [
                "ActsHuksJsApi8FuncTest.hap"
            ],
            "type": "AppInstallKit",
            "cleanup-apps": true
        },
        {
            "type": "ShellKit",
            "run-command": [
                "hidumper -s 3301 -a -t",
                "power-shell wakeup"
            ]
        }
    ]
}