{
  "description": "Configuration for hjunit demo Tests",
  "driver": {
    "type": "OHJSUnitTest",
    "test-timeout": "1800000",
    "shell-timeout": "900000",
    "testcase-timeout": "600000",
    "bundle-name": "com.test.huks.etsapi23.group",
    "module-name": "entry_test"
  },
  "kits": [
    {
      "test-file-name": [
        "ActsHuksGroupFuncTest.hap"
      ],
      "type": "AppInstallKit",
      "cleanup-apps": true
    },
    {
      "type": "ShellKit",
      "run-command": [
        "hidumper -s 3301 -a -t",
        "power-shell wakeup"
      ]
    }
  ]
}