{
    "description": "Configuration for ActsHuksNdkTest Tests",
    "driver": {
        "type": "OHJSUnitTest",
        "test-timeout": "180000",
        "shell-timeout": "600000",
        "bundle-name": "com.acts.huks.ndkapi8",
        "module-name": "entry_test",
        "testcase-timeout": 60000
    },
    "kits": [
        {
            "test-file-name": [
                "ActsHuksNdkTest.hap",
                "ActsHuksNdkMainTest.hap"
            ],
            "type": "AppInstallKit",
            "cleanup-apps": true
        },
        {
            "type": "ShellKit",
            "run-command": [
                "hidumper -s 3301 -a -t",
                "power-shell wakeup"
            ]
        }
    ]
}