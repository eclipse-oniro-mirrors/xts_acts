{
    "description": "Configuration for hjunit demo Tests",
    "driver": {
        "type": "OHJSUnitTest",
        "bundle-name": "com.openharmony.cryptoFramework3.static",
        "module-name": "entry",
        "test-timeout": "1800000",
        "shell-timeout": "1500000",
        "testcase-timeout": "900000"
    },
    "kits": [{
        "test-file-name": [
            "ActsCryptoFrameworkThreeJSFuncStaticTest.hap"
        ],
        "type": "AppInstallKit",
        "cleanup-apps": true
    },{
        "type": "ShellKit",
        "run-command": [
            "power-shell wakeup",
            "uinput -T -m 300 2700 280 200",
            "power-shell setmode 602"
        ]
    }]
}