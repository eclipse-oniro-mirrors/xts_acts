          } else {
            ret = compareString(vector.expectPriKeyPem, p12.privateKey as string);
          }
        } else {
          console.log("############# no priKey");
          ret = false;
        }
      }

      if (checkCaCert) {
        if (p12.cert) {
          let x509Cert: cert.X509Cert = await this.createCaX509Cert(algName, isPem);
          let pubKey: cryptoFramework.PubKey|undefined = p12.cert?.getPublicKey();
          if (pubKey) {
            await x509Cert.verify(pubKey);
          }
          console.log("############### ca cert verify success");
        } else {
          console.log("############# no ca cert");
          ret = false;
        }
      }
      if (checkOtherCerts) {
        if (p12.otherCerts) {
          index = 0;
          let len: int|undefined = p12.otherCerts?.length;
          console.log("############# otherCerts count:" + len);
          /* 创建p12时，如果未添加公钥证书，只添加了私钥，和1本私钥签发的其他证书1本其他证书，p12解析后该私钥签发的其他证书存放在证书里面 */
          if (isPem && len && len < vector.otherCertPem.length) {
            index += 1;
          } else if (!isPem && len && len < vector.otherCertDer.length) {
            index += 1;
          }
          /* 创建p12时，如果未添加私钥，添加了公钥证书，p12解析后该公钥证书存放在其他证书里面 */
          if (!checkPriKey && checkOtherCerts && !isOnlyOtherCerts) {
            let caCert: cert.X509Cert | undefined | null = p12.otherCerts?.shift();
            len = p12.otherCerts?.length;
            if (caCert !== undefined && caCert) {
              let x509Cert: cert.X509Cert = await this.createCaX509Cert(algName, isPem);
              await x509Cert.verify(caCert.getPublicKey());
              console.log("############### other ca cert verify success");
            } else {
              console.log("############# no other ca cert");
              ret = false;
            }
          }
          if (len && len > 0) {
            for (let i = 0; i < len; i++) {
              let x509Cert: cert.X509Cert = await this.createOtherX509Cert(algName, index, isPem);
              if (p12.otherCerts) {
                let otherCert = p12.otherCerts?.[i]; // p12.otherCerts![i]
                let pubKey: cryptoFramework.PubKey|undefined = otherCert?.getPublicKey();
                if (pubKey) {
                  await x509Cert.verify(pubKey);
                }
                index += 1;
              }

            }
          }
          console.log("############# index:" + index);
        } else {
          console.log("############# no other certs");
          ret = false;
        }
      }
      console.log("############# check end");
    } catch (error: BusinessError) {
      console.error(`checkPkcs12 ${error}, error code: ${error.code} otherCert:${index}`);
      ret = false;
    }

    return ret;
  }

  async createCaX509Cert(algName: string, isPem: boolean = true): Promise<cert.X509Cert> {
    let encodingBlob: cert.EncodingBlob;
    let vector: OH_Pkcs12Data = this.getVectorData(algName);
    if (isPem) {
      encodingBlob = {
        data: stringTouInt8Array(vector.certPem),
        encodingFormat: cert.EncodingFormat.FORMAT_PEM
      };
    } else {
      encodingBlob = {
        data: new Uint8Array(vector.certDer),
        encodingFormat: cert.EncodingFormat.FORMAT_DER
      };
    }
    try {
      let x509Cert: cert.X509Cert = await cert.createX509Cert(encodingBlob);
      return x509Cert;
    } catch (error: BusinessError) {
      console.error(`createCaX509Cert ${error}, error code: ${error.code}`);
      throw error as Error;
    }
  }

  async createOtherX509Cert(algName: string, index: int, isPem: boolean = true): Promise<cert.X509Cert> {
    let encodingBlob: cert.EncodingBlob;
    let vector: OH_Pkcs12Data = this.getVectorData(algName);
    if (isPem) {
      encodingBlob = {
        data: stringTouInt8Array(vector.otherCertPem[index]),
        encodingFormat: cert.EncodingFormat.FORMAT_PEM
      };
    } else {
      encodingBlob = {
        data: new Uint8Array(vector.otherCertDer[index]),
        encodingFormat: cert.EncodingFormat.FORMAT_DER
      };
    }
    try {
      let x509Cert: cert.X509Cert = await cert.createX509Cert(encodingBlob);
      return x509Cert;
    } catch (error: BusinessError) {
      console.error(`createOtherX509Cert ${error}, error code: ${error.code}`);
      throw error as Error;
    }
  }
}

async function createX509Cert(inStream: string): Promise<cert.X509Cert> {
  let encodingBlob: cert.EncodingBlob = {
    data: stringTouInt8Array(inStream),
    encodingFormat: cert.EncodingFormat.FORMAT_PEM

  };
  let x509Cert: cert.X509Cert = await cert.createX509Cert(encodingBlob);

  return x509Cert;
}

async function getRevokedCerts(crl: cert.X509CRL): Promise<cert.X509CRLEntry[]> {
  return new Promise<cert.X509CRLEntry[]>((resolve, reject) => {
    crl.getRevokedCerts((err, crlEntries) => {
      if (err || crlEntries === undefined) {
        reject(err as Error);
      } else {
        resolve(crlEntries);
      }
    });
  });
}

function setPbesParams(saltLen: int, iterations: int = 2048,
  encryptionAlgorithm: cert.PbesEncryptionAlgorithm = cert.PbesEncryptionAlgorithm.AES_256_CBC): cert.PbesParams {
  try {
    let PbesParams: cert.PbesParams = {
      saltLen: saltLen,
      iterations: iterations,
      encryptionAlgorithm: encryptionAlgorithm
    };
    return PbesParams;
  } catch (error: BusinessError) {
    console.error(`setPbesParams ${error}, error code: ${error.code}`);
    throw error as Error;
  }
}

function setMacParams(saltLen: int, iterations: int = 2048,
  macDigestAlgorithm: cert.Pkcs12MacDigestAlgorithm = cert.Pkcs12MacDigestAlgorithm.SHA256): cert.Pkcs12CreationConfig {
  try {
    let config: cert.Pkcs12CreationConfig = {
      password: "123456",
      macSaltLen: saltLen,
      macIterations: iterations,
      macDigestAlgorithm: macDigestAlgorithm
    };
    return config;
  } catch (error: BusinessError) {
    console.error(`setMacParams ${error}, error code: ${error.code}`);
    throw error as Error;
  }
}

