{
    "description": "Configuration for hjunit demo Tests",
    "driver": {
        "type": "OHJSUnitTest",
        "test-timeout": "300000",
        "shell-timeout": "60000000",
		"testcase-timeout": "180000000",
        "bundle-name": "com.example.linkenhancetest.static",
        "module-name": "entry"
    },
    "kits": [
        {
            "test-file-name": [
                "ActslinkEnhanceStaticTest.hap"
            ],
            "type": "AppInstallKit",
            "cleanup-apps": true
        },{
            "type": "ShellKit",
            "run-command": [
                "power-shell wakeup",
                "uinput -T -m 300 2700 280 200",
                "power-shell setmode 602"
            ]
        }
    ]
}
