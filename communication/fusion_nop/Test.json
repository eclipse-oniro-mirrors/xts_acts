{
    "description": "Configuration for hjunit demo Tests",
    "driver": {
        "type": "OHJSUnitTest",
        "test-timeout": "180000",
        "shell-timeout": "180000",
        "bundle-name": "ohos.acts.communication.fusionConnectivity.partnerAgentnop",
        "module-name": "entry_test",
		"testcase-timeout": "20000"
    },
    "kits": [
        {
            "test-file-name": [
                "ActsFusionNopTest.hap"
            ],
            "type": "AppInstallKit",
            "cleanup-apps": true
        },{
            "type": "ShellKit",
            "run-command": [
                "param set persist.usb.setting.gadget_conn_prompt false",
                "param set persist.skip_scb_charging 1",
                "wait 5",
                "power-shell wakeup",
                "uinput -T -m 300 2600 280 200",
                "wait 5",
                "uinput -T -m 300 2600 280 200",
                "wait 5",
                "uinput -T -m 300 1279 280 200",
                "uinput -K -d 2054 -u 2054",
                "hidumper -s 3301 -a -t",
                "uitest dumpLayout"
            ],
            "teardown-command": [
                "param set persist.usb.setting.gadget_conn_prompt true"
            ]
        }
    ]
}