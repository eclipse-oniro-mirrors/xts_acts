{
    "description": "Configuration for ActsPrintNdkTest Tests",
    "driver": {
        "type": "OHJSUnitTest",
        "test-timeout": "600000",
        "bundle-name": "com.acts.actsprinterrorcodenopertest",
        "module-name": "entry_test",
        "shell-timeout": "600000"
    },
    "kits": [{
        "test-file-name": [
            "ActsPrintErrorCodeNoPerTest.hap",
            "ActsPrintErrorCodeNoPerMainTest.hap"
        ],
        "type": "AppInstallKit",
        "cleanup-apps": true
    }, {
        "type": "ShellKit",
        "run-command": [
            "power-shell wakeup",
            "power-shell setmode 602"
        ]
    }]
}
