{
  "description": "Configuration for ActsTelephonyNdk20Test Tests",
  "driver": {
    "type": "OHJSUnitTest",
    "test-timeout": "180000",
    "shell-timeout": "180000",
    "bundle-name": "com.acts.telenoperndk20test",
    "module-name": "entry_test"
  },
  "kits": [
    {
      "test-file-name": [
        "ActsTelNDK20NoPerTest.hap",
        "ActsTelNDK20NoPerMainTest.hap"
      ],
      "type": "AppInstallKit",
      "cleanup-apps": true
    },
    {
      "type": "ShellKit",
      "run-command": [
        "power-shell wakeup",
        "power-shell setmode 602"
      ]
    }
  ]
}
