{
    "description": "Configuration for hjunit demo Tests",
    "driver": {
        "type": "OHJSUnitTest",
        "test-timeout": "300000",
        "bundle-name": "ohos.acts.multimedia.image.imagePicture.Static",
        "module-name": "entry",
        "shell-timeout": "300000",
        "testcase-timeout": "5000"
    },
    "kits": [
        {
            "test-file-name": [
                "ActsImagePictureStaticTest.hap"
            ],
            "type": "AppInstallKit",
            "cleanup-apps": true
        },
        {
            "type": "ShellKit",
            "run-command": [
                "mkdir -p /data/app/el2/100/base/ohos.acts.multimedia.image.imagePicture.Static/haps/entry/files/",
                "chmod -R 666 /data/app/el2/100/base/ohos.acts.multimedia.image.imagePicture.Static/haps/entry/files/*",
                "mkdir -p /data/app/el2/100/base/ohos.acts.multimedia.image.imagePicture.Static/haps/entry/cache/",
                "chmod -R 777 /data/app/el2/100/base/ohos.acts.multimedia.image.imagePicture.Static/haps/entry/*"
            ]
        },
        {
            "type": "PushKit",
            "pre-push": [],
            "push": [
                "./resource/image/HEIFISOMultiChannelBaseColor0512V12.heic ->/data/app/el2/100/base/ohos.acts.multimedia.image.imagePicture.Static/haps/entry/files",
                "./resource/image/0801_sdr2.heic ->/data/app/el2/100/base/ohos.acts.multimedia.image.imagePicture.Static/haps/entry/files",
                "./resource/image/all_auxtype.heic ->/data/app/el2/100/base/ohos.acts.multimedia.image.imagePicture.Static/haps/entry/files",
                "./resource/image/AllAuxiliaryPictures.jpg ->/data/app/el2/100/base/ohos.acts.multimedia.image.imagePicture.Static/haps/entry/files",
                "./resource/image/CUVAHdrMulti.jpg ->/data/app/el2/100/base/ohos.acts.multimedia.image.imagePicture.Static/haps/entry/files",
                "./resource/image/HDRVividSingleLayer.heic ->/data/app/el2/100/base/ohos.acts.multimedia.image.imagePicture.Static/haps/entry/files",
                "./resource/image/JPEGISOSingle.jpg ->/data/app/el2/100/base/ohos.acts.multimedia.image.imagePicture.Static/haps/entry/files",
                "./resource/image/AllAuxiliaryPictures_exclude_fragment.jpg ->/data/app/el2/100/base/ohos.acts.multimedia.image.imagePicture.Static/haps/entry/files",
                "./resource/image/JpegFragmentMap.jpg ->/data/app/el2/100/base/ohos.acts.multimedia.image.imagePicture.Static/haps/entry/files",
                "./resource/image/test.jpg ->/data/app/el2/100/base/ohos.acts.multimedia.image.imagePicture.Static/haps/entry/files",
                "./resource/image/test.png ->/data/app/el2/100/base/ohos.acts.multimedia.image.imagePicture.Static/haps/entry/files",
                "./resource/image/testExif.jpg ->/data/app/el2/100/base/ohos.acts.multimedia.image.imagePicture.Static/haps/entry/files",
                "./resource/image/GainMap.dat ->/data/app/el2/100/base/ohos.acts.multimedia.image.imagePicture.Static/haps/entry/files",
                "./resource/image/UnrefocusMap.dat ->/data/app/el2/100/base/ohos.acts.multimedia.image.imagePicture.Static/haps/entry/files",
                "./resource/image/FragmentMap.dat ->/data/app/el2/100/base/ohos.acts.multimedia.image.imagePicture.Static/haps/entry/files"
            ]
        },
        {
            "type": "ShellKit",
            "run-command": [
                "chmod -R 777 /data/app/el2/100/base/ohos.acts.multimedia.image.imagePicture.Static/haps/entry/files/*",
                "power-shell wakeup",
                "power-shell setmode 602"
            ]
        }
    ]
}
