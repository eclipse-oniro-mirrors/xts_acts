{
    "description": "Configuration for Image Tests",
    "driver": {
        "type": "OHJSUnitTest",
        "test-timeout": "1500000",
        "shell-timeout": "1500000",
        "bundle-name": "ohos.acts.multimedia.image.Packing.Static",
        "testcase-timeout": 180000,
        "module-name": "entry"
    },
    "kits": [
        {
            "test-file-name": [
                "ActsImagePackingJsStaticTest.hap"
            ],
            "type": "AppInstallKit",
            "cleanup-apps": true
        },
        {
            "type": "ShellKit",
            "run-command": [
                "mkdir -p /data/app/el2/100/base/ohos.acts.multimedia.image.Packing.Static/haps/entry/files/",
                "chmod -R 666 /data/app/el2/100/base/ohos.acts.multimedia.image.Packing.Static/haps/entry/files/*",
                "chmod -R 777 /data/app/el2/100/base/ohos.acts.multimedia.image.Packing.Static/haps/entry/*"
            ],
            "teardown-command": []
        },
        {
            "type": "PushKit",
            "pre-push": [],
            "push": [
                "./resource/image/test.png ->/data/app/el2/100/base/ohos.acts.multimedia.image.Packing.Static/haps/entry/files",
                "./resource/image/moving_test.gif ->/data/app/el2/100/base/ohos.acts.multimedia.image.Packing.Static/haps/entry/files",
                "./resource/image/test.bmp ->/data/app/el2/100/base/ohos.acts.multimedia.image.Packing.Static/haps/entry/files",
                "./resource/image/test.jpg ->/data/app/el2/100/base/ohos.acts.multimedia.image.Packing.Static/haps/entry/files",
                "./resource/image/test.ico ->/data/app/el2/100/base/ohos.acts.multimedia.image.Packing.Static/haps/entry/files",
                "./resource/image/test.svg ->/data/app/el2/100/base/ohos.acts.multimedia.image.Packing.Static/haps/entry/files",
                "./resource/image/test.dng ->/data/app/el2/100/base/ohos.acts.multimedia.image.Packing.Static/haps/entry/files",
                "./resource/image/test.jpeg ->/data/app/el2/100/base/ohos.acts.multimedia.image.Packing.Static/haps/entry/files",
                "./resource/image/test.webp ->/data/app/el2/100/base/ohos.acts.multimedia.image.Packing.Static/haps/entry/files",
                "./resource/image/HDRVividSingleLayer.heic ->/data/app/el2/100/base/ohos.acts.multimedia.image.Packing.Static/haps/entry/files"
            ]
        },
        {
            "type": "ShellKit",
            "run-command": []
        }
    ]
}