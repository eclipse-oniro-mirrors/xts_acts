{
  "description": "Configuration for ActsCapabilityTest Tests",
  "driver": {
    "type": "OHJSUnitTest",
    "test-timeout": "600000",
    "shell-timeout": "600000",
    "bundle-name": "com.example.ActsCapabilityTest",
    "module-name": "entry_test"
  },
  "kits": [
    {
      "test-file-name": [
        "ActsCapabilityTest.hap",
        "ActsCapabilityHapTest.hap"
      ],
      "type": "AppInstallKit",
      "cleanup-apps": true
    },{
      "type": "ShellKit",
      "run-command": [
        "power-shell wakeup",
        "power-shell setmode 602"
      ]
    }
  ]
}
