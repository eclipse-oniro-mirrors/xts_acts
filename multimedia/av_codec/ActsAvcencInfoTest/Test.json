{
  "description": "Configuration for ActsAvcencInfoTest Tests",
  "driver": {
    "type": "OHJSUnitTest",
    "test-timeout": "600000",
    "shell-timeout": "600000",
    "bundle-name": "com.example.avcenc_info",
    "module-name": "entry_test"
  },
  "kits": [
    {
      "test-file-name": [
        "ActsAvcencInfoHapTest.hap",
        "ActsAvcencInfoMainTest.hap"
      ],
      "type": "AppInstallKit",
      "cleanup-apps": true
    },
    {
      "type": "ShellKit",
      "run-command": [
        "power-shell wakeup", 
        "power-shell setmode 602"
      ]
    }
  ]
}
