{
    "description": "Configuration for ActsAudioTestTest Tests",
    "driver": {
        "type": "OHJSUnitTest",
        "test-timeout": "600000",
        "shell-timeout": "600000",
        "bundle-name": "com.example.actsaudiotesttest",
        "module-name": "entry_test",
        "testcase-timeout": "60000"
    },
    "kits": [
        {
            "test-file-name": [
                "ActsAudioHapTest.hap",
                "ActsAudioHapMainTest.hap"
            ],
            "type": "AppInstallKit",
            "cleanup-apps": true
        },{
            "type": "ShellKit",
            "run-command": [
                "mkdir -p /data/app/el2/100/base/com.example.actsaudiotesttest/haps/entry_test/files",
                "chmod 777 -R /data/app/el2/100/base/com.example.actsaudiotesttest/haps/entry_test",
                "power-shell wakeup",
                "power-shell setmode 602"
            ]
        },{
            "type": "PushKit",
            "pre-push": [],
            "push": [
                "./resource/media/codec/audio_test/aac_2c_44100hz_199k.pcm ->/data/app/el2/100/base/com.example.actsaudiotesttest/haps/entry_test/files/"
            ]
        }
    ]
}
