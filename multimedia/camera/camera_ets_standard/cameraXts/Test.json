{
    "description": "Configuration for camerastandard Tests",
    "driver": {
        "type": "OHJSUnitTest",
        "test-timeout": "3000000",
        "bundle-name": "ohos.acts.multimedia.camera.xts",
        "shell-timeout": "600000",
        "testcase-timeout": 300000,
        "module-name": "entry_test"
    },
    "kits": [
        {
            "type": "ShellKit",
            "run-command": [
                "hilog -Q pidoff",
                "rm -rf /storage/media/100/local/files/Videos/*",
                "power-shell wakeup",
                "power-shell timeout -o 2147483647",
                "uinput -T -m 300 2700 280 200",
                "power-shell setmode 602"
            ],
            "teardown-command": [
                "power-shell setmode 600",
                "rm -rf /data/app/el2/100/database/ohos.acts.multimedia.camera.xts/*",
                "kill -9 `pidof ohos.acts.multimedia.camera.xts`",
                "scanner",
                "sleep 5"
            ]
        },
        {
            "test-file-name": [
                "ActsCameraXtsTest.hap"
            ],
            "type": "AppInstallKit",
            "cleanup-apps": true
        }
    ]
}