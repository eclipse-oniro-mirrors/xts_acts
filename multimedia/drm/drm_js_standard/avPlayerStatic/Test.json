{
    "description": "Configuration for hjunit demo Tests",
    "driver": {
        "type": "OHJSUnitTest",
        "test-timeout": "360000",
        "bundle-name": "com.example.myapplication.static",
        "module-name": "entry",
        "shell-timeout": "360000",
        "testcase-timeout": "70000"
    },
    "kits": [{
        "test-file-name": [
            "ActsDRMStaticTest.hap"
        ],
        "type": "AppInstallKit",
        "cleanup-apps": true
    },{
        "type": "ShellKit",
        "run-command": [
            "power-shell wakeup",
            "uinput -T -m 300 2700 280 200",
            "power-shell setmode 602"
        ]
    }]
}