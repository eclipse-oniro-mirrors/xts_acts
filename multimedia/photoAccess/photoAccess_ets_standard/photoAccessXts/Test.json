{
  "description": "Configuration for photoAccess Tests",
  "driver": {
    "type": "OHJSUnitTest",
    "test-timeout": "180000",
    "bundle-name": "ohos.acts.multimedia.photoaccess.xts",
    "module-name": "entry_test",
    "shell-timeout": "600000",
    "testcase-timeout": 120000
  },
  "kits": [
    {
      "type": "ShellKit",
      "pre-push": [],
      "run-command": [
        "rm -rf /storage/media/100/local/files/Pictures",
        "rm -rf /storage/media/100/local/files/Videos",
        "rm -rf /storage/media/100/local/files/Camera",
        "rm -rf /storage/media/100/local/files/Audios",
        "rm -rf /storage/media/100/local/files/Docs/Documents/*",
        "rm -rf /storage/media/100/local/files/Docs/Documents/.*",
        "rm -rf /storage/media/100/local/files/entry_test/*",
        "rm -rf /storage/media/100/local/files/Photo/*",
        "sleep 5",
        "rm -rf /data/app/el2/100/database/ohos.acts.multimedia.photoaccess.xts/*",
        "kill -9 `pidof ohos.acts.multimedia.photoaccess.xts`"
      ]
    },
    {
      "test-file-name": [
        "ActsPhotoAccessXtsTest.hap"
      ],
      "type": "AppInstallKit",
      "cleanup-apps": true
    },
    {
      "type": "ShellKit",
      "pre-push": [],
      "run-command": [
        "param set persist.ace.testmode.enabled 1",
        "power-shell wakeup",
        "uinput -T -m 300 2700 280 200",
        "power-shell setmode 602",
        "mkdir -p /storage/media/100/local/files/Pictures",
        "mkdir -p /storage/media/100/local/files/Videos",
        "mkdir -p /storage/media/100/local/files/Camera",
        "mkdir -p /storage/media/100/local/files/Audios",
        "mkdir /storage/media/100/local/files/Pictures/Screenshots",
        "mkdir -p /data/app/el2/100/base/ohos.acts.multimedia.photoaccess.xts/haps/entry_test/files",
        "mkdir -p /data/app/el2/100/base/ohos.acts.multimedia.photoaccess.xts/haps/entry_test/photos",
        "chmod 777 -R /data/app/el2/100/base/ohos.acts.multimedia.photoaccess.xts/haps/entry_test",
        "chmod -R 777 /storage/media/100/local/files/*",
        "chmod -R 777 /data/service/el2/100/hmdfs/account/files/*"
      ]
    },
    {
      "type": "PushKit",
      "pre-push": [],
      "push": [
        "./resource/photoaccess/01.jpg ->/data/app/el2/100/base/ohos.acts.multimedia.photoaccess.xts/haps/entry_test/files/",
        "./resource/photoaccess/01.mp4 ->/data/app/el2/100/base/ohos.acts.multimedia.photoaccess.xts/haps/entry_test/files/",
        "./resource/photoaccess/cloneTest.jpg ->/data/app/el2/100/base/ohos.acts.multimedia.photoaccess.xts/haps/entry_test/photos/",
        "./resource/photoaccess/cloneTest.mp4 ->/data/app/el2/100/base/ohos.acts.multimedia.photoaccess.xts/haps/entry_test/photos/",
        "./resource/photoaccess/getTest.jpg ->/data/app/el2/100/base/ohos.acts.multimedia.photoaccess.xts/haps/entry_test/photos/",
        "./resource/photoaccess/getTest.mp4 ->/data/app/el2/100/base/ohos.acts.multimedia.photoaccess.xts/haps/entry_test/photos/",
        "./resource/photoaccess/getThumb.jpg ->/data/app/el2/100/base/ohos.acts.multimedia.photoaccess.xts/haps/entry_test/photos/",
        "./resource/photoaccess/getThumb.mp4 ->/data/app/el2/100/base/ohos.acts.multimedia.photoaccess.xts/haps/entry_test/photos/",
        "./resource/photoaccess/modifyCb01.jpg ->/data/app/el2/100/base/ohos.acts.multimedia.photoaccess.xts/haps/entry_test/photos/",
        "./resource/photoaccess/modifyCb01.mp4 ->/data/app/el2/100/base/ohos.acts.multimedia.photoaccess.xts/haps/entry_test/photos/",
        "./resource/photoaccess/modifyCb02.jpg ->/data/app/el2/100/base/ohos.acts.multimedia.photoaccess.xts/haps/entry_test/photos/",
        "./resource/photoaccess/modifyCb02.mp4 ->/data/app/el2/100/base/ohos.acts.multimedia.photoaccess.xts/haps/entry_test/photos/",
        "./resource/photoaccess/modifyCb03.jpg ->/data/app/el2/100/base/ohos.acts.multimedia.photoaccess.xts/haps/entry_test/photos/",
        "./resource/photoaccess/modifyCb03.mp4 ->/data/app/el2/100/base/ohos.acts.multimedia.photoaccess.xts/haps/entry_test/photos/",
        "./resource/photoaccess/modifyCb04.jpg ->/data/app/el2/100/base/ohos.acts.multimedia.photoaccess.xts/haps/entry_test/photos/",
        "./resource/photoaccess/modifyCb04.mp4 ->/data/app/el2/100/base/ohos.acts.multimedia.photoaccess.xts/haps/entry_test/photos/",
        "./resource/photoaccess/modifyCb05.jpg ->/data/app/el2/100/base/ohos.acts.multimedia.photoaccess.xts/haps/entry_test/photos/",
        "./resource/photoaccess/modifyCb05.mp4 ->/data/app/el2/100/base/ohos.acts.multimedia.photoaccess.xts/haps/entry_test/photos/",
        "./resource/photoaccess/modifyCb06.mp4 ->/data/app/el2/100/base/ohos.acts.multimedia.photoaccess.xts/haps/entry_test/photos/",
        "./resource/photoaccess/modifyCb07.jpg ->/data/app/el2/100/base/ohos.acts.multimedia.photoaccess.xts/haps/entry_test/photos/",
        "./resource/photoaccess/modifyCb07.mp4 ->/data/app/el2/100/base/ohos.acts.multimedia.photoaccess.xts/haps/entry_test/photos/",
        "./resource/photoaccess/modifyPro01.jpg ->/data/app/el2/100/base/ohos.acts.multimedia.photoaccess.xts/haps/entry_test/photos/",
        "./resource/photoaccess/modifyPro01.mp4 ->/data/app/el2/100/base/ohos.acts.multimedia.photoaccess.xts/haps/entry_test/photos/",
        "./resource/photoaccess/modifyPro02.jpg ->/data/app/el2/100/base/ohos.acts.multimedia.photoaccess.xts/haps/entry_test/photos/",
        "./resource/photoaccess/modifyPro02.mp4 ->/data/app/el2/100/base/ohos.acts.multimedia.photoaccess.xts/haps/entry_test/photos/",
        "./resource/photoaccess/modifyPro03.jpg ->/data/app/el2/100/base/ohos.acts.multimedia.photoaccess.xts/haps/entry_test/photos/",
        "./resource/photoaccess/modifyPro03.mp4 ->/data/app/el2/100/base/ohos.acts.multimedia.photoaccess.xts/haps/entry_test/photos/",
        "./resource/photoaccess/modifyPro04.jpg ->/data/app/el2/100/base/ohos.acts.multimedia.photoaccess.xts/haps/entry_test/photos/",
        "./resource/photoaccess/modifyPro04.mp4 ->/data/app/el2/100/base/ohos.acts.multimedia.photoaccess.xts/haps/entry_test/photos/",
        "./resource/photoaccess/modifyPro05.jpg ->/data/app/el2/100/base/ohos.acts.multimedia.photoaccess.xts/haps/entry_test/photos/",
        "./resource/photoaccess/modifyPro05.mp4 ->/data/app/el2/100/base/ohos.acts.multimedia.photoaccess.xts/haps/entry_test/photos/",
        "./resource/photoaccess/modifyPro06.jpg ->/data/app/el2/100/base/ohos.acts.multimedia.photoaccess.xts/haps/entry_test/photos/",
        "./resource/photoaccess/modifyPro06.mp4 ->/data/app/el2/100/base/ohos.acts.multimedia.photoaccess.xts/haps/entry_test/photos/",
        "./resource/photoaccess/modifyPro07.jpg ->/data/app/el2/100/base/ohos.acts.multimedia.photoaccess.xts/haps/entry_test/photos/",
        "./resource/photoaccess/modifyPro07.mp4 ->/data/app/el2/100/base/ohos.acts.multimedia.photoaccess.xts/haps/entry_test/photos/",
        "./resource/photoaccess/testGetAsset.jpg ->/data/app/el2/100/base/ohos.acts.multimedia.photoaccess.xts/haps/entry_test/photos/",
        "./resource/photoaccess/testGetAsset.mp4 ->/data/app/el2/100/base/ohos.acts.multimedia.photoaccess.xts/haps/entry_test/photos/"
      ]
    },
    {
      "type": "ShellKit",
      "run-command": [
        "hilog -Q pidoff",
        "hilog -Q domainoff",
        "hilog -b D -D 0xD002B70"
      ],
      "teardown-command": [
        "power-shell setmode 600",
        "hilog -Q pidon",
        "hilog -Q domainon",
        "rm -rf /storage/media/100/local/files/Pictures",
        "rm -rf /storage/media/100/local/files/Videos",
        "rm -rf /storage/media/100/local/files/Camera",
        "rm -rf /storage/media/100/local/files/Audios",
        "rm -rf /storage/media/100/local/files/Docs/Documents/*",
        "rm -rf /storage/media/100/local/files/Docs/Documents/.*",
        "rm -rf /storage/media/100/local/files/entry_test/*",
        "rm -rf /storage/media/100/local/files/Photo/*",
        "sleep 5",
        "rm -rf /data/app/el2/100/database/com.ohos.medialibrary.medialibrarydata/*",
        "kill -9 `pidof com.ohos.medialibrary.medialibrarydata`"
      ]
    }
  ]
}