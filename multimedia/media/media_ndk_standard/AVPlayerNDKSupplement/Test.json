{
    "description": "Configuration for OhAudioNdk Tests",
    "driver": {
        "type": "OHJSUnitTest",
        "test-timeout": "600000",
        "shell-timeout": "600000",
        "bundle-name": "com.example.avplayerndk",
        "module-name": "entry_test",
        "testcase-timeout": 30000
    },
    "kits": [
        {
            "test-file-name": [
                "ActsOhAVPlayerSupNDKMainTest.hap",
                "ActsOhAVPlayerSupNDKTest.hap"
            ],
            "type": "AppInstallKit",
            "cleanup-apps": true
        },
        {
            "type": "ShellKit",
            "run-command": [
                "remount",
                "mkdir -p /data/app/el2/100/base/com.example.avplayerndk/haps/entry_test/files",
                "chmod 777 -R /data/app/el2/100/base/com.example.avplayerndk/haps/"
            ]
        },
        {
            "type": "PushKit",
            "push": [
                "./resource/audio/H264_AAC.mp4 -> /data/app/el2/100/base/com.example.avplayerndk/haps/entry_test/files/",
                "./resource/media/codec/demuxer/srt_3200.srt -> /data/app/el2/100/base/com.example.avplayerndk/haps/entry_test/files/",
                "./resource/media/AVPlayer/H264_AAC_2audio.mp4 -> /data/app/el2/100/base/com.example.avplayerndk/haps/entry_test/files/"
            ]
        },
        {
            "type": "ShellKit",
            "run-command": [
                "power-shell wakeup",
                "uinput -T -m 300 2700 280 200",
                "power-shell setmode 602"
            ]
        }
    ]
}