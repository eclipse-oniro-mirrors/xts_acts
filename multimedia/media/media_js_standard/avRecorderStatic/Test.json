{
    "description": "Configuration for hjunit demo Tests",
    "driver": {
        "type": "OHJSUnitTest",
        "test-timeout": "180000",
        "bundle-name": "ohos.acts.multimedia.audio.avrecorder.static",
        "module-name": "entry",
        "shell-timeout": "600000",
        "testcase-timeout": "180000"
    },
    "kits": [{
        "test-file-name": [
            "ActsAvRecorderJsStaticTest.hap"
        ],
        "type": "AppInstallKit",
        "cleanup-apps": true
    },
    {
        "type": "ShellKit",
        "run-command": [
            "param set persist.ace.testmode.enabled 1",
            "power-shell wakeup",
            "uinput -T -m 300 2700 280 200",
            "power-shell setmode 602"
        ],
        "teardown-command": [
            "power-shell setmode 600"
        ]
    }]
}
