{
    "description": "Configuration for hjunit demo Tests",
    "driver": {
        "type": "OHJSUnitTest",
        "test-timeout": "180000",
        "bundle-name": "ohos.acts.multimedia.avscreencapturerecorder.static",
        "module-name": "entry",
        "shell-timeout": "600000",
        "testcase-timeout": "180000"
    },
    "kits": [{
        "test-file-name": [
            "ActsAvScreenCaptureRecorderJsStaticTest.hap"
        ],
        "type": "AppInstallKit",
        "cleanup-apps": true
    },
    {
        "type": "ShellKit",
        "run-command": [
            "remount",
            "param set persist.ace.testmode.enabled 1",
            "power-shell wakeup",
            "power-shell timeout -o 2147483647",
            "power-shell setmode 602",
            "mkdir -p /data/app/el2/100/base/ohos.acts.multimedia.avscreencapturerecorder.static/haps/entry/files",
            "chmod 777 -R /data/app/el2/100/base/ohos.acts.multimedia.avscreencapturerecorder.static/haps/entry"
        ],
        "teardown-command": [
            "power-shell setmode 600"
        ]
    },
    {
        "type": "PushKit",
        "push": [
            "./resource/media/codec/audio_encoder/f32le_44100_2_dayuhaitang.pcm -> /data/app/el2/100/base/ohos.acts.multimedia.avscreencapturerecorder.static/haps/entry/files/"
        ]
    }]
}
