{
    "description": "Configuration for audio manager Tests",
    "driver": {
        "type": "OHJSUnitTest",
        "test-timeout": "1500000",
        "testcase-timeout": "15000",
        "bundle-name": "com.example.audiohaptic.Static",
        "module-name": "entry",
        "shell-timeout": "60000"
    },
    "kits": [
        {
            "test-file-name": [
                "ActsAudioHapticJsStaticTest.hap"
            ],
            "type": "AppInstallKit",
            "cleanup-apps": true
        },
        {
            "type": "ShellKit",
            "run-command": [
                "power-shell wakeup",
                "uinput -T -m 300 2700 280 200",
                "power-shell setmode 602"
            ],
            "teardown-command": [
                "power-shell setmode 600"
            ]
        },
        {
            "type": "PushKit",
            "pre-push": [],
            "push": []
        }
    ]
}