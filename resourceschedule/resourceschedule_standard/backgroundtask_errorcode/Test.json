{
    "description": "Configuration for ActsBackgroundProcessManagerNdkTest Tests",
    "driver": {
        "type": "OHJSUnitTest",
        "test-timeout": "180000",
        "bundle-name": "com.acts.backgroundtaskerrorcodetest",
        "module-name": "entry_test",
        "shell-timeout": "600000",
        "testcase-timeout": 60000
    },
    "kits": [{
        "test-file-name": [
            "ActsBackgroundTaskErrorTest.hap",
            "ActsBackgroundTaskErrorMainTest.hap"
        ],
        "type": "AppInstallKit",
        "cleanup-apps": true
    }, {
        "type": "ShellKit",
        "run-command": [
            "power-shell wakeup",
            "power-shell setmode 602"
        ]
    }, {
        "type": "ShellKit",
        "teardown-command": [
            "rm -rf data/test"
        ]
    }
]
}
