{
    "description": "Configuration for hjunit demo Tests",
    "driver": {
        "type": "OHJSUnitTest",
        "test-timeout": "60000",
        "shell-timeout": "60000",
        "bundle-name": "com.acts.mechanicmanager.static",
        "module-name": "entry",
        "testcase-timeout": "70000"
    },
    "kits": [{
        "test-file-name": [
            "ActsmechanicManagerTestStatic.hap"
        ],
        "type": "AppInstallKit",
        "cleanup-apps": true
    },{
        "type": "ShellKit",
        "run-command": [
            "reboot",
            "sleep 5",
            "power-shell wakeup",
            "uinput -T -m 300 2700 280 200",
            "power-shell setmode 602",
            "power-shell timeout -o 86400000",
            "hidumper -s 3301 -a -t"
        ],
        "teardown-command": [
            "reboot",
            "sleep 5",
            "power-shell wakeup",
            "power-shell setmode 602"
        ]
    }]
}
