{
    "description": "Configuration for devicemanager Tests",
    "driver": {
        "type": "OHJSUnitTest",
        "test-timeout": "180000",
        "bundle-name": "ohos.acts.actsdevicemanagertest",
        "module-name": "entry_test",
        "shell-timeout": "600000",
        "testcase-timeout": 60000
    },
    "kits": [{
        "test-file-name": [
            "ActsdeviceManager20Test.hap",
            "ActsdeviceManagerMain20Test.hap"
        ],
        "type": "AppInstallKit",
        "cleanup-apps": true
    }, {
        "type": "ShellKit",
        "run-command": [
            "power-shell wakeup",
            "power-shell setmode 602"
        ]

    }]
}
