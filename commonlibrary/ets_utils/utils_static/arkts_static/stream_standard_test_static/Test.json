{
    "description": "stream test",
    "driver": {
        "type": "OHJSUnitTest",
        "test-timeout": "800000",
        "shell-timeout": "800000",
		"testcase-timeout": "15000",
        "bundle-name": "com.stream.standard.static",
		"module-name": "entry"
    },
    "kits": [
        {
            "test-file-name": [
                "ActsStreamStaticTest.hap"
            ],
            "type": "AppInstallKit",
            "cleanup-apps": true
        },{
            "type": "ShellKit",
            "run-command": [
                "power-shell wakeup",
                "power-shell setmode 602"
            ]
        }
    ]
}