{
    "description": "spec Tests",
    "driver": {
        "type": "OHJSUnitTest",
        "test-timeout": "180000",
        "shell-timeout": "180000",
        "testcase-timeout": "15000",
        "bundle-name": "spec.standard.test.static",
        "module-name": "entry"
    },
    "kits": [
        {
            "test-file-name": [
                "ActsSpecStaticTest.hap"
            ],
            "type": "AppInstallKit",
            "cleanup-apps": true
        },
        {
            "type": "ShellKit",
            "run-command": [
                "power-shell wakeup",
                "power-shell setmode 602"
            ]
        }
    ]
}