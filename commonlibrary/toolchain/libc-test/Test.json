                "mkdir /tmp",
                "mkdir /dev/shm",
                "tar -xf /data/local/tmp/libc-test.tar -C /data/local/tmp/",
                "tar -xf /data/local/tmp/libc-test-lib.tar -C /data/local/tmp/",
                "chmod a+x -R /data/local/tmp/libc-test-lib",
                "chmod a+x -R /data/local/tmp/libc-test",
                "mount -o rw,remount /",
                "mv /data/local/tmp/libc-test/zh_CN /tmp/zh_CN",
                "mkdir /data/tests",
                "mkdir /data/tests/libc-test",
                "mkdir /data/tests/libc-test/src",
                "mkdir -p /data/tests/libc-test/src/rpath_lib/rpath_support_A",
                "mkdir -p /data/tests/libc-test/src/rpath_lib/rpath_support_B",
                "mkdir -p /data/tests/libc-test/src/rpath_support_C",
