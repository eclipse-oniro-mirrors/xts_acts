{
    "description": "Configuration for hjunit demo Tests",
    "driver": {
        "type": "OHJSUnitTest",
        "test-timeout": "180000",
        "shell-timeout": "180000",
        "testcase-timeout": "9000",
        "bundle-name": "com.acts.actsCallerInfoTest",
        "module-name": "entry_test"
    },
    "kits": [
        {
            "test-file-name": [
                "ActsCallerInfoAssist01Test.hap",
                "ActsCallerInfoTest.hap"
            ],
            "type": "AppInstallKit",
            "cleanup-apps": true
        },
        {
            "type": "ShellKit",
            "run-command": [
                "power-shell wakeup",
                "uinput -T -m 300 1279 280 200",
                "uinput -T -d 300 600 -m 300 600 300 100 -u 300 100",
                "power-shell timeout -o 86400000",
                "power-shell setmode 602",
                "param set persist.sys.abilityms.support.ecologicalrulemgrservice false"
            ],
            "teardown-command": [
                "param set persist.sys.abilityms.support.ecologicalrulemgrservice true"
            ]
        }
    ]
}