{
    "description": "Configuration for aceceshi Tests",
    "driver": {
        "type": "OHJSUnitTest",
        "test-timeout": "300000",
        "shell-timeout": "300000",
        "bundle-name": "com.acts.ability.multiinstaceerrcodetest",
        "module-name": "entry_test",
        "testcase-timeout": "15000"
    },
    "kits": [
        {
            "test-file-name": [
                "ActsBaseErrorCodeMethodTest.hap",
                "ActsmultiInstancEerrCodeMain.hap",
                "ActsBaseErrorRely.hap",
                "ActsMultiInstancEerrCodeTest.hap"
              ],
            "type": "AppInstallKit",
            "cleanup-apps": true
        },
        {
            "type": "ShellKit",
            "run-command": [
                "power-shell wakeup",
                "uinput -T -m 300 1279 280 200",
                "uinput -T -d 300 600 -m 300 600 300 100 -u 300 100",
                "power-shell timeout -o 86400000",
                "power-shell setmode 602"
            ]
        }
    ]
}