{
    "description": "Configuration for hjunit demo Tests",
    "driver": {
        "type": "OHJSUnitTest",
        "test-timeout": "180000",
        "bundle-name": "com.acts.example.requestdialog.static",
        "module-name": "entry",
        "shell-timeout": "600000",
        "testcase-timeout": "180000"
    },
    "kits": [{
        "test-file-name": [
            "ActsRequestDialogStaticTest.hap"
        ],
        "type": "AppInstallKit",
        "cleanup-apps": true
    },{
        "type": "ShellKit",
        "run-command": [
            "power-shell wakeup",
            "uinput -T -m 300 2700 280 200",
            "power-shell setmode 602",
            "remount",
            "mkdir /data/test"
        ]
    }, {
        "type": "PushKit",
        "push": [
            "ActsRequestDialogStaticTest.hap->/data/test/ActsRequestDialogStaticTest.hap"
        ]
    }, {
        "type": "ShellKit",
        "run-command": [
            "chmod 644 /data/test/*.hap"
        ]
    }]
}