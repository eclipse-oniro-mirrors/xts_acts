{
    "description": "Configuration for aceceshi Tests",
    "driver": {
        "type": "OHJSUnitTest",
        "test-timeout": "600000",
        "bundle-name": "com.example.windowchangedvisibilitytest",
        "module-name": "entry_test",
        "shell-timeout": "600000",
        "testcase-timeout": "30000"
    },
    "kits": [
		{
            "test-file-name": [
                "WindowChangedVisibilityTest.hap",
                "windowVisibilityRely.hap",
                "windowVisibilityTest.hap"
            ],
            "type": "AppInstallKit",
            "cleanup-apps": true
        },
        {
            "type": "ShellKit",
            "run-command": [
                "param set persist.sys.suspend_manager_enabled false",
                "reboot",
                "wait 5",
                "power-shell wakeup",
                "uinput -T -m 300 2700 280 200",
                "power-shell setmode 602",
                "hilog -Q pidoff",
                "hilog -Q domainoff",
                "hilog -b D",
                "setenforce 0"
            ],
            "teardown-command": [
                "param set persist.sys.suspend_manager_enabled true",
                "power-shell setmode 601",
                "reboot"
            ]
        }
    ]
}