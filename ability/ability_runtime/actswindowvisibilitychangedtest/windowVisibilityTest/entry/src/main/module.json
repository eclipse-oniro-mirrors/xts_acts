{
  "module": {
    "name": "entry",
    "type": "entry",
    "description": "$string:module_desc",
    "mainElement": "EntryAbility",
    "deviceTypes": [
      "default",
      "tablet"
    ],
    "deliveryWithInstall": true,
    "installationFree": false,
    "pages": "$profile:main_pages",
    "abilities": [
      {
        "name": "EntryAbility",
        "srcEntry": "./ets/entryability/EntryAbility.ts",
        "description": "$string:EntryAbility_desc",
        "icon": "$media:icon",
        "label": "$string:EntryAbility_label",
        "startWindowIcon": "$media:icon",
        "startWindowBackground": "$color:start_window_background",
        "exported": true,
        "skills": [
          {
            "entities": [
              "entity.system.home"
            ],
            "actions": [
              "action.system.home"
            ]
          }
        ]
      },
      {
        "name": "EntryAbility1",
        "srcEntry": "./ets/entryability1/EntryAbility1.ts",
        "description": "$string:EntryAbility1_desc",
        "icon": "$media:icon",
        "label": "$string:EntryAbility1_label",
        "startWindowIcon": "$media:icon",
        "startWindowBackground": "$color:start_window_background",
        "exported": true
      }
    ],
    "extensionAbilities": [
      {
        "name": "SingleServiceExtAbility",
        "srcEntry": "./ets/singleserviceextability/SingleServiceExtAbility.ts",
        "description": "$string:SingleServiceExtAbility_desc",
        "type": "service",
        "icon": "$media:icon",
        "label": "$string:SingleServiceExtAbility_label",
        "exported": true
      },
      {
        "name": "MultipleServiceExtAbility",
        "srcEntry": "./ets/multipleserviceextability/MultipleServiceExtAbility.ts",
        "description": "$string:MultipleServiceExtAbility_desc",
        "type": "service",
        "icon": "$media:icon",
        "label": "$string:MultipleServiceExtAbility_label",
        "exported": true
      },
      {
        "name": "SingleServiceExtAbility1",
        "srcEntry": "./ets/singleserviceextability1/SingleServiceExtAbility1.ts",
        "description": "$string:SingleServiceExtAbility1_desc",
        "type": "service",
        "icon": "$media:icon",
        "label": "$string:SingleServiceExtAbility1_label",
        "exported": true
      },
      {
        "name": "singleserviceextability2",
        "srcEntry": "./ets/singleserviceextability2/singleserviceextability2.ts",
        "description": "$string:singleserviceextability2_desc",
        "type": "service",
        "icon": "$media:icon",
        "label": "$string:singleserviceextability2_label",
        "exported": true
      }
    ],
    "requestPermissions": [
      {
        "name": "ohos.permission.SYSTEM_FLOAT_WINDOW"
      },
      {
        "name": "ohos.permission.START_ABILITIES_FROM_BACKGROUND"
      },
      {
        "name": "ohos.permission.START_INVISIBLE_ABILITY"
      }
    ]
  }
}