{
    "description": "Configuration for aceceshi Tests",
    "driver": {
        "type": "OHJSUnitTest",
        "test-timeout": "600000",
        "bundle-name": "com.example.mainhap",
        "module-name": "entry_test",
        "shell-timeout": "600000",
        "testcase-timeout": 25000
    },
    "kits": [
        {
            "type": "ShellKit",
            "run-command": [
                "power-shell wakeup",
                "uinput -T -m 300 1279 280 200",
                "uinput -T -d 300 600 -m 300 600 300 100 -u 300 100",
                "power-shell timeout -o 86400000",
                "power-shell setmode 602"
            ]
        },
		{
            
            "test-file-name": [
                "ActsForResultTestRely.hap",
                "ActsForResultTestNoPermissionRely.hap",
                "ActsuiExtensionForResultRely.hap",
                "ActsuiExtensionForResultTest.hap"
            ],
            "type": "AppInstallKit",
            "cleanup-apps": true
        }
    ]
}