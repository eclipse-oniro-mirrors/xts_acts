{
    "description": "Configuration for hjunit demo Tests",
    "driver": {
        "type": "OHJSUnitTest",
        "test-timeout": "180000",
        "shell-timeout": "180000",
        "bundle-name": "com.example.GetAllWindowStages",
        "module-name": "entry_test",
        "testcase-timeout": "15000"
    },
    "kits": [
        {
            "test-file-name": [
                "ActsGetAllWindowStagesTest.hap",
                "applicationModule.hap",
                "entryModule.hap"
            ],
            "type": "AppInstallKit",
            "cleanup-apps": true
        },{
            "type": "ShellKit",
            "run-command": [
                "power-shell wakeup",
                "power-shell setmode 602",
                "uinput -T -m 300 1279 280 200",
                "uinput -T -d 1070 1070 -u 1070 1070",
                "power-shell timeout -o 8640000"
            ]
        }
    ]
}