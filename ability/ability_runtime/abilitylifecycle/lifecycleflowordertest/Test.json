{
    "description": "Configuration for hjunit Tests",
    "driver": {
        "type": "OHJSUnitTest",
        "test-timeout": "600000",
        "shell-timeout": "60000",
        "bundle-name": "com.acts.lifecycleflowordertest",
        "module-name": "entry_test",
        "testcase-timeout": 60000
    },
    "kits": [
        {
            "test-file-name": [
                "ActsLifeCycleFlowOrderTest.hap",
                "ActsLifeCycleFlowOrderTestDemo.hap"
            ],
            "type": "AppInstallKit",
            "cleanup-apps": true
        },
        {
          "type": "ShellKit",
          "run-command": [
              "param set persist.sys.abilityms.support.ecologicalrulemgrservice false",
              "power-shell wakeup",
              "uinput -T -m 300 1279 280 200",
              "uinput -T -d 300 600 -m 300 600 300 100 -u 300 100",
              "power-shell timeout -o 86400000",
              "power-shell setmode 602"
          ],
          "teardown-command": [
              "param set persist.sys.abilityms.support.ecologicalrulemgrservice true"
          ]
        }
    ]
}