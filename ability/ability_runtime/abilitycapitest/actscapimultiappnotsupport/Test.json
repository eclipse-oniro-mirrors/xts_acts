{
    "description": "Configuration for ActsCapiMultiAppNotSupportTest Tests",
    "driver": {
        "type": "OHJSUnitTest",
        "test-timeout": "600000",
        "bundle-name": "com.acts.actscapimultiappnotsupport",
        "module-name": "entry_test",
        "shell-timeout": "600000",
        "testcase-timeout":"30000",
        "compile-mode":"esmodule"
    },
    "kits": [{
        "test-file-name": [
            "ActsCapiMultiAppNotSupportTest.hap",
            "ActsCapiMultiAppNotSupportMainTest.hap"
        ],
        "type": "AppInstallKit",
        "cleanup-apps": true
    }, {
        "type": "ShellKit",
        "run-command": [
            "power-shell wakeup",
            "power-shell setmode 602"
        ]
    }]
}
