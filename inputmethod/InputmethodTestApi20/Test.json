{
    "description": "Configuration for ActsInputmethodTest Tests",
    "driver": {
        "type": "OHJSUnitTest",
        "test-timeout": "180000",
        "bundle-name": "com.acts.actsinputmethodtest",
        "module-name": "entry_test",
        "shell-timeout": "600000",
        "testcase-timeout": 60000
    },
    "kits": [{
        "test-file-name": [
            "ActsInputmethodApi20Test.hap",
            "ActsInputmethodMainApi20Test.hap"

        ],
        "type": "AppInstallKit",
        "cleanup-apps": true
    }, {
        "type": "ShellKit",
        "run-command": [
            "power-shell wakeup",
            "power-shell setmode 602",
            "power-shell timeout -o 86400000",
            "hidumper -s 3301 -a -t"
        ]

    }]
}
