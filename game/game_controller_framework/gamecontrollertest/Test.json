{
    "description": "Configuration for gamecontroller Tests",
    "driver": {
        "type": "OHJSUnitTest",
        "test-timeout": "180000",
        "bundle-name": "ohos.acts.game.gamecontrollertest",
        "module-name": "entry_test",
        "shell-timeout": "600000",
        "testcase-timeout": 60000
    },
    "kits": [{
        "test-file-name": [
            "ActsGameControllerTest.hap",
            "ActsGameControllerMainTest.hap"
        ],
        "type": "AppInstallKit",
        "cleanup-apps": true
    }, {
        "type": "ShellKit",
        "run-command": [
            "power-shell wakeup",
            "power-shell setmode 602"
        ]

    }]
}
