{
    "description": "Configuration for ActsOsAccountNdkTest Tests",
    "driver": {
        "type": "OHJSUnitTest",
        "test-timeout": "180000",
        "bundle-name": "com.acts.actspasteboarderrortest",
        "module-name": "entry_test",
        "shell-timeout": "600000",
        "testcase-timeout": 70000
    },
    "kits": [{
        "test-file-name": [
            "ActsPasteboardErrorTest.hap",
            "ActsPasteboardErrorMainTest.hap"
        ],
        "type": "AppInstallKit",
        "cleanup-apps": true
    }, {
        "type": "ShellKit",
        "run-command": [
            "power-shell wakeup",
            "power-shell setmode 602"
        ]
    }]
}
