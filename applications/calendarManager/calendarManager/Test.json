{
    "description": "Configuration for settings Tests",
    "driver": {
        "type": "OHJSUnitTest",
        "test-timeout": "180000",
        "bundle-name": "com.test.calendarManager",
        "module-name": "phone",
        "shell-timeout": "600000"
    },
    "kits": [{
        "test-file-name": [
            "ActsCalendarManagerEtsTest.hap"
        ],
        "type": "AppInstallKit",
        "cleanup-apps": true
    }, {
        "type": "ShellKit",
        "run-command": [
            "power-shell wakeup",
            "power-shell setmode 602",
			"uinput -T -m 425 1000 425 500 400"
        ]       
    }]
}