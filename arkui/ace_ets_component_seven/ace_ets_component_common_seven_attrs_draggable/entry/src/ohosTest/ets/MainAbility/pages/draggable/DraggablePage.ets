					.commonStyle()
					.attributeModifier(this._draggableFormLinkModifier)
			}
			if (this.targetView == 'Hyperlink') {
				Hyperlink('https://example.com/', 'Go to the developer website') {
				}
					.commonStyle()
					.attributeModifier(this._draggableHyperlinkModifier)
