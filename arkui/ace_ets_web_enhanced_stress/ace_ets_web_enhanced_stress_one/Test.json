{
    "description": "Configuration for ActsAceWebStressTest Tests",
    "driver": {
        "type": "OHJSUnitTest",
        "test-timeout": "18000000",
        "bundle-name": "com.aceweb.stress.test",
        "module-name": "com.aceweb.stress.test",
        "shell-timeout": "4200000",
        "testcase-timeout": 4000000
    },
    "kits": [{
        "test-file-name": [
            "ActsAceWebStressTest.hap"
        ],
        "type": "AppInstallKit",
        "cleanup-apps": true
    }, {
        "type": "ShellKit",
        "run-command": [
            "power-shell wakeup",
            "power-shell setmode 602",
            "cd ./storage/media/100/local/files/Documents/",
            "touch 1.json"
        ]
    }]
}