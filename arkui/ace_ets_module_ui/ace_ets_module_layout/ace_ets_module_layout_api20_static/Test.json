{
  "description": "Configuration for hjunit demo Tests",
  "driver": {
    "type": "OHJSUnitTest",
    "test-timeout": "180000",
    "bundle-name": "com.arkui.ace.layout.api20.static",
    "module-name": "entry",
    "shell-timeout": "600000",
    "testcase-timeout": "180000"
  },
  "kits": [
    {
      "test-file-name": [
        "ActsAceEtsModuleLayoutApi20StaticTest.hap"
      ],
      "type": "AppInstallKit",
      "cleanup-apps": true
    },
    {
      "type": "ShellKit",
      "run-command": [
        "power-shell wakeup",
        "power-shell setmode 602",
        "power-shell timeout -o 864000000"
      ]
    }
  ]
}