{
    "description": "Configuration for hjunit demo Tests",
    "driver": {
        "type": "OHJSUnitTest",
        "test-timeout": "180000",
        "bundle-name": "com.open.harmony.nowearevent.static",
        "module-name": "entry",
        "shell-timeout": "600000",
        "testcase-timeout": "600000"
    },
    "kits": [{
        "test-file-name": [
            "ActsAceEtsModuleCommonEventsNoWearApi14StaticTest.hap"
        ],
        "type": "AppInstallKit",
        "cleanup-apps": true
    },{
        "type": "ShellKit",
        "run-command": [
            "power-shell wakeup",
            "uinput -T -m 300 2700 280 200",
            "power-shell setmode 602",
            "power-shell timeout -o 864000000"
        ]
    }]
}