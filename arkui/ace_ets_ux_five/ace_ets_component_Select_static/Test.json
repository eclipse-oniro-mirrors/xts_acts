{
  "description": "Configuration for hjunit demo Tests",
  "driver": {
    "type": "OHJSUnitTest",
    "test-timeout": "600000",
    "shell-timeout": "600000",
    "bundle-name": "com.example.Select.static",
    "module-name": "entry"
  },
  "kits": [
    {
      "test-file-name": [
        "ActsSelectStaticTest.hap"
      ],
      "type": "AppInstallKit",
      "cleanup-apps": true
    },
    {
      "type": "ShellKit",
      "run-command": [
        "power-shell wakeup",
        "power-shell setmode 602"
      ]
    }
  ]
}
