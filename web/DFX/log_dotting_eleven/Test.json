{
    "description": "Configuration for hjunit demo Tests",
    "driver": {
        "type": "OHJSUnitTest",
        "test-timeout": "180000",
        "bundle-name": "com.open.harmony.logdottingeleventest",
        "module-name": "entry_test",
        "shell-timeout": "600000",
        "testcase-timeout": 180000
    },
    "kits": [{
            "test-file-name": [
	            "ActsLogDottingCppElevenTest.hap",
                "ActsWebLogDottingElevenTest.hap"
            ],
            "type": "AppInstallKit",
            "cleanup-apps": true
        },
        {
            "type": "ShellKit",
            "run-command": [
                "power-shell wakeup",
                "power-shell setmode 602"
            ]
        }
    ]
}