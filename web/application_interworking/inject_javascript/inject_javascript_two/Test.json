{
    "description": "Configuration for native image Tests",
    "driver": {
        "type": "OHJSUnitTest",
        "test-timeout": "600000",
        "shell-timeout": "600000",
        "bundle-name": "com.open.harmony.injectjavascripttwotest",
        "module-name": "entry_test",
        "testcase-timeout": "70000"
    },
    "kits": [
        {
            "test-file-name": [
                "ActsWebInjectJavacriptTwoTest.hap",
                "ActsWebInjectJavacriptTwoMainTest.hap"
            ],
            "type": "AppInstallKit",
            "cleanup-apps": true
        },{
            "type": "ShellKit",
            "run-command": [
                "power-shell wakeup",
                "power-shell setmode 602",
                "power-shell timeout -o 864000000"
            ]
        }
    ]
}
