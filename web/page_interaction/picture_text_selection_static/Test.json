{
  "description": "Configuration for hjunit demo Tests",
  "driver": {
    "type": "OHJSUnitTest",
    "test-timeout": "180000",
    "shell-timeout": "180000",
    "bundle-name": "com.pageInteraction.test.static",
    "module-name": "entry",
    "testcase-timeout": "180000"
  },
  "kits": [
    {
      "test-file-name": [
        "ActsWebPictureTextSelectionStaticTest.hap"
      ],
      "type": "AppInstallKit",
      "cleanup-apps": true
    },
    {
      "type": "ShellKit",
      "run-command": [
        "power-shell wakeup",
        "power-shell setmode 602"
      ]
    }
  ]
}