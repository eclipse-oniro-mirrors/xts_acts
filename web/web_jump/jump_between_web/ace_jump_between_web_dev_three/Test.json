{
    "description": "Configuration for hjunit demo Tests",
    "driver": {
        "type": "OHJSUnitTest",
        "test-timeout": "600000",
        "bundle-name": "com.open.harmony.actsacejumpbetweenwebdevthreetest",
        "module-name": "phone",
        "shell-timeout": "600000",
        "testcase-timeout": "70000"
    },
    "kits": [{
        "test-file-name": [
            "ActsWebJumpBetweenWebDevThreeTest.hap"
        ],
        "type": "AppInstallKit",
        "cleanup-apps": true,
        "run-command": [
            "wait 5",
            "power-shell wakeup",
            "uinput -T -m 300 2700 280 200",
            "wait 5",
            "uinput -T -m 300 2700 280 200",
            "power-shell timeout -o 864000000"
        ]
    }]
}