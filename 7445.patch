<script>
  var reportUrl = "/openharmony/xts_acts/pulls/7445/scan_reports";
  var taskId = "";
  var $tabs = $('#pull-request-tabs .item');
  var $diffUrl = "/openharmony/xts_acts/pulls/7445/check_diff";
  var $goReview = $('.js-go-review');
  
  function goScanTab(tab) {
    var $item = $('.item[data-tab=' + tab + ']')
    if (!$item.length) {
      return;
    }
  
    $tabs.tab('change tab', tab);
    reqData(tab, function() {
      $('.tab[data-tab=' + tab + ']').find('.loader-wrap').removeClass('active')
      $('html, body').animate({
        scrollTop: $item.offset().top
      }, 500);
    })
  }
  
  function loop() {
    $.get($diffUrl)
      .done(function (res) {
        if (res.calculate_diff === false) {
          location.reload();
        } else {
          setTimeout(function () {
            loop()
          }, 3000)
        }
      });
  }
  
  $(function() {
    var searchParams = new URLSearchParams(window.location.search);
    var tab = searchParams.get('tab');
    if (false) {
      loop()
    }
  
    window.Gitee.initPullRequests();
    new Gitee.PullRequest();
    new Gitee.PullRequestCheckBoxes({
      username: window.gon.username,
      projectName: window.gon.project_name,
      canModifyPullRequestMain: false
    })
    $('.general-evaluate').popup({ popup: '.scan-rate-popup', position: 'right center' });
    $tabs.tab().on('click', function () {
      var $this = $(this);
      if ($this.data('tab') === 'pull-request-details') {
        $this.addClass('active');
        window.eventHub.$emit('switchNav', $this.data('nav'));
      } else {
        $('body').removeClass('fullscreen-checks');
        $('.pull-request__container').addClass('hide-sidebox');
      }
    });
    $goReview.on('click', function () {
      window.eventHub.$emit('switchNav', 'comments');
      $this = $(this);
      let connent_id = $this.attr('data-value')
      let offset = 100;
      offset += document.getElementById('git-header-nav').offsetHeight
      const $pullRequestTabs = document.getElementById('pull-request-tabs')
      if ($pullRequestTabs) {
        offset += $pullRequestTabs.offsetHeight
      }
      let $commentId = $("#comment-"+connent_id)
      if($commentId){
        setTimeout(function () {
          $('body,html').animate({
              scrollTop: $commentId.offset().top-offset
            },500
          );
          $commentId.addClass('comment-focus')
          },100)
  
        $(document).on('click', function () {
          $commentId.removeClass('comment-focus');
        });
      }
    });
    if (tab) {
      goScanTab(tab)
    }
  
    $('#pull-request-tabs').on('click', '.tab-item', function () {
      goScanTab($(this).data('tab'))
    })
  
    $('.commit-select-wrap').dropdown({
      onChange: function (value) {
        var type = $(this).closest('.gitee-scan.tab').data('tab')
        $('#' + type + '-scan-wrap .pagination-wrap').html('')
        reqData(type, function () {
          $('.tab[data-tab=' + type + ']').find('.loader-wrap').removeClass('active')
        }, value)
      }
    })
    if (window.gon.nps) {
      var nps1 = getNps({ codeName: "1" });
      document.body.addEventListener('click', function(e) {
        if (e.target && e.target.closest('[data-handler=bootstrap-markdown-cmdSuggestion]')) {
          nps1.addTick()
        }
      }, true);
    }
  });
  
  window._web_tips = {};
  window._web_tips.fullscreenTips = '全屏查看';
  window._web_tips.fullscreenExitTips = '退出全屏';
  window._web_tips.diffCollapsedTips = '差异被折叠，点击展开';
  window.gon.pullId = 8020355;
  window.gon.diffsViewMode = null;
  window.gon.viewedFilesPath = "/openharmony/xts_acts/pulls/7445/update_viewed_file";
  window.gon.sourcesProjectPath = "/openharmony/xts_acts/autocomplete_sources";
  window.gon.createCommentsPath = "/openharmony/xts_acts/notes";
  window.gon.project_is_closed = false
  window.gon.project_issue_readonly = false
  window.gon.pullRequestPath = "/openharmony/xts_acts/pulls/7445";
  window.gon.pull_request_review_path = "/openharmony/xts_acts/pulls/7445/pull_request_review";
  window.gon.pull_request_review_recheck_path = "/openharmony/xts_acts/pulls/7445/pull_request_review/recheck";
  window.gon.modify_pull_request = false
  window.gon.readonly_pull_request = false
  
  $(function() {
    // 只读模式 使用 jquery 禁用按钮
    const i18_readonly_pull_request_tips = `当前仓库已关闭 Pull Request 新建、更新功能`
    window.gon.i18_readonly_pull_request_tips = i18_readonly_pull_request_tips
  
    if(window.gon.readonly_pull_request){
      const $PullRequestSubject = $('.pull-request__subject')
  
      // PR 操作 （编辑、取消草稿、关闭、回滚） 等按钮禁用
      $PullRequestSubject.find('.pr-action').each(function() {
         if($(this).hasClass('disabled')){
          return
         }
        $(this).addClass("disabled").css("pointer-events", "none").attr("href", "javascript:;")
        const tooltips = $(this).parent().attr("data-tooltip")
        if(!tooltips){
           $(this).wrap('<div class="d-inline-block" data-tooltip="' + i18_readonly_pull_request_tips + '"></div>')
        }
      });
  
      // PR 审查 合并 等按钮禁用
      const $PullRequestContainer = $('.pull-request__subject, .pull-request__merge-widget')
      $PullRequestContainer.find(".ui.button").each(function() {
        if($(this).hasClass('js-note-reaction') || $(this).hasClass('js-btn-copy-clone-url')){
          return
        }
        $(this).addClass("disabled")
        const tooltips = $(this).parent().attr("data-tooltip")
        if(!tooltips){
           $(this).wrap('<div class="d-inline-block" data-tooltip="' + i18_readonly_pull_request_tips + '"></div>')
        }
      });
  
      // PR侧边栏 编辑按钮禁用
      const $PullRequestSidebar = $(".pull-request__sidebar")
      $PullRequestSidebar.find(('.field .label .ui.dropdown')).each(function() {
         if($(this).hasClass('disabled')){
          return
         }
         $(this).addClass('disabled')
         $(this).find(".icon-edit").css('display', 'inline-block')
         $(this).wrap('<div class="d-inline-block float-right" data-tooltip="' + i18_readonly_pull_request_tips + '"></div>')
      });
  
      // PR侧边栏 自动化 checkbox 按钮禁用
      $PullRequestSidebar.find('.auto-processing .checkbox').each(function() {
         if($(this).hasClass('disabled')){
          return
         }
         $(this).addClass('disabled')
         $(this).wrap('<div class="d-inline-block" data-tooltip="' + i18_readonly_pull_request_tips + '"></div>')
      });
       // PR侧边栏 审查、测试人数 下拉按钮禁用
      $PullRequestSidebar.find('.dropdown.min-reviewers-dropdown').each(function() {
         if($(this).hasClass('disabled')){
          return
         }
         $(this).addClass('disabled')
         $(this).wrap('<div class="d-inline-block" data-tooltip="' + i18_readonly_pull_request_tips + '"></div>')
         $(this).find('.icon-return').css("cssText", "display: inline-block!important")
      });
    }
  });
</script>
