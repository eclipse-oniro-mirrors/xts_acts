        popup.html("关闭 Diff 评论");
      } else {
        popup.html("展开 Diff 评论");
      }
    }
  });
  
  $toggleNoteBtn.on('click', function () {
    notesVisible = !notesVisible
    if (window.eventHub) {
      window.eventHub.$emit('hide-comment-groups', notesVisible);
    } else {
      $('tr.notes_holder').each(function () {
        if ($(this).is(':visible') !== notesVisible) {
          Notes.toggleNotesVisible($(this));
        }
      });
    }
    $(this).popup('hide').popup('show');
  });
</script>

