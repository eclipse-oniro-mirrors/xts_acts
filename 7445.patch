<script>
  (function() {
    $(function() {
      var $cpBranchDropdown, $cpModal, $cpOk, $cpProjectDropdown, $sourceProjectDropdown, evtHub, filterXSS, initBranchDropdown, initProjectDropdown, initSourceProjectDropdown, storeCommits, submitting;
      $cpModal = $('#cherry-pick__modal');
      $sourceProjectDropdown = $cpModal.find('.source-project-dropdown');
      $cpProjectDropdown = $cpModal.find('.project-dropdown');
      $cpBranchDropdown = $cpModal.find('.branch-dropdown');
      $cpOk = $cpModal.find('.ok');
      filterXSS = window.filterXSS;
      evtHub = window.eventHub || {
        $on: $.noop,
        $emit: $.noop
      };
      storeCommits = [];
      submitting = false;
      if (!$cpModal.length) {
        return;
      }
      evtHub.$on('cherry-pick-submit', function(payload) {
        storeCommits = payload.commits;
        submitting = false;
        $cpOk.removeClass('disabled loading');
        return $cpModal.modal('show');
      });
      initSourceProjectDropdown = function() {
        $sourceProjectDropdown.addClass('loading');
        return $.get('/openharmony/xts_acts/cherry_pick_targets', function(data) {
          $sourceProjectDropdown.removeClass('loading');
          data = data.map(function(p) {
            return {
              value: p.cherry_pick_path,
              name: filterXSS(p.path_with_namespace)
            };
          });
          $sourceProjectDropdown.dropdown('setup menu', {
            values: data
          });
          return $sourceProjectDropdown.removeClass('loading');
        });
      };
      initProjectDropdown = function() {
        $cpProjectDropdown.addClass('loading');
        return $.get('/openharmony/xts_acts/pull_request_targets', function(data) {
          $cpProjectDropdown.removeClass('loading');
          data = data.projects.map(function(p) {
            return {
              value: p.id,
              name: filterXSS(p.path_with_namespace)
            };
          });
          $cpProjectDropdown.dropdown('setup menu', {
            values: data
          });
          return $cpProjectDropdown.removeClass('loading');
        });
      };
      initBranchDropdown = function() {
        var path;
        $cpBranchDropdown.addClass('loading');
        path = $cpProjectDropdown.dropdown('get text');
        return $.get("/" + path + "/pull_request_branches", function(data) {
          var nVal, oVal;
          $cpBranchDropdown.removeClass('loading');
          oVal = $cpBranchDropdown.dropdown('get value');
          nVal = '';
          data = data.branches.map(function(b) {
            if (oVal === b.name) {
              nVal = oVal;
            }
            return {
              value: filterXSS(b.name),
              name: filterXSS(b.name)
            };
          });
          return $cpBranchDropdown.dropdown('setup menu', {
            values: data
          }).dropdown('set selected', nVal || data[0].value);
        });
      };
      $sourceProjectDropdown.one('click', initSourceProjectDropdown).dropdown({
        forceSelection: false
      });
      $cpProjectDropdown.one('click', initProjectDropdown).dropdown({
        forceSelection: false,
        onChange: function() {
          $cpBranchDropdown.off('click', initBranchDropdown);
          return initBranchDropdown();
        }
      });
      $cpBranchDropdown.one('click', initBranchDropdown).dropdown({
        forceSelection: false
      });
      return $cpModal.modal({
        closable: false,
        autofocus: false,
        onApprove: function() {
          submitting = true;
          $cpOk.addClass('disabled loading');
          $.ajax({
            url: $sourceProjectDropdown.dropdown('get value'),
            method: 'POST',
            data: {
              pr_id: '8020355',
              target_project_id: $cpProjectDropdown.dropdown('get value'),
              target_branch_name: $cpBranchDropdown.dropdown('get value'),
              cherry_pick_oids: storeCommits
            }
          }).done(function(res) {
            var data;
            data = res.data;
            if (data && data.status === 'success') {
              return window.location.href = data.new_pr_path;
            }
          }).fail(function(err) {
            var data;
            data = err.responseJSON;
            if (data && data.data && data.data.message) {
              return Flash.error(data.data.message);
            }
          }).always(function() {
            submitting = false;
            return $cpOk.removeClass('disabled loading');
          });
          return false;
        },
        onDeny: function() {
          return !submitting;
        },
        onHide: function() {
          return !submitting;
        }
      });
    });
  
  }).call(this);
</script>

